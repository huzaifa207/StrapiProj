{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "name": "Doctors",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "contactDetails": {
      "type": "component",
      "repeatable": false,
      "component": "contact-details.contact-details"
    },
    "clinicInfo": {
      "type": "component",
      "repeatable": false,
      "component": "clinic-info.clinic-info"
    },
    "education": {
      "type": "component",
      "repeatable": true,
      "component": "eduction.education"
    },
    "experience": {
      "type": "component",
      "repeatable": true,
      "component": "experience.experience"
    },
    "biography": {
      "type": "text"
    },
    "services": {
      "collection": "services",
      "via": "doctors",
      "dominant": true
    },
    "freeConsultaion": {
      "type": "boolean",
      "default": false
    },
    "pricePerHour": {
      "type": "integer",
      "min": 100,
      "default": 100
    },
    "awards": {
      "type": "component",
      "repeatable": true,
      "component": "awards.awards"
    },
    "memberships": {
      "type": "component",
      "repeatable": true,
      "component": "membership.membership"
    },
    "registrations": {
      "type": "component",
      "repeatable": true,
      "component": "registrations.registrations"
    },
    "patientCount": {
      "type": "integer",
      "default": 0
    },
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 5
    },
    "reviews": {
      "via": "doctor",
      "collection": "reviews"
    },
    "socialLinks": {
      "type": "component",
      "repeatable": false,
      "component": "social-links.social-links"
    },
    "patients": {
      "collection": "patient"
    },
    "userRole": {
      "via": "doctor",
      "plugin": "users-permissions",
      "model": "user"
    },
    "specializations": {
      "collection": "specialization",
      "via": "doctors",
      "dominant": true
    }
  }
}
