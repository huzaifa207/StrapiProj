{
  "kind": "collectionType",
  "collectionName": "patients",
  "info": {
    "name": "Patient",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "bloodGroup": {
      "type": "string"
    },
    "contactDetails": {
      "type": "component",
      "repeatable": false,
      "component": "contact-details.contact-details"
    },
    "reviews": {
      "collection": "reviews",
      "via": "patient"
    },
    "favorites": {
      "collection": "doctors"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "userRole": {
      "via": "patient",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
